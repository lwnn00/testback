{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/_utils/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html"
    }
  ]
}